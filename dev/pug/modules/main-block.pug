            +b.btn-wrap
                +b.btn-style.js-play-video
                    i.fa.fa-play
            +b.btn-wrap
                +b.btn-style.js-stop-video
                    i.fa.fa-stop
            +b.btn-wrap
                +b.btn-style.js-backward-video
                    i.fa.fa-backward
            +b.btn-wrap
                +b.btn-style.js-forward-video
                    i.fa.fa-forward
            